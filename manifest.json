{
   "manifest_version": 2,
   "name": "Track Covid-19 -Nepal",
   "description": "Quick Lunch Traversy Media",
   "version": "1.0.0",
   "icons": {
      "128": "icon_128.png"
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "permissions": [
      "activeTab",
      "https://cdn.jsdelivr.net/npm/vue/dist/vue.js"
   ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' https://coronavirus-monitor-v2.p.rapidapi.com https://ajax.googleapis.com https://cdn.jsdelivr.net; object-src 'self'"
}